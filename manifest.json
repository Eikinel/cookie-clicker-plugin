{
    "name": "Cookie Clicker Share",
    "version": "0.1",
    "description": "Import/Export Cookie Clicker saves between devices",
    "permissions": [
      "activeTab", "tabs", "declarativeContent", "storage", "identity",
      "http://orteil.dashnet.org/cookieclicker/", "https://orteil.dashnet.org/cookieclicker/"
    ],
    "oauth2": {
      "client_id": "457978225628-17m0dautimr8bh3cc3ba37uio6km8epq.apps.googleusercontent.com",
      "scopes": [
        "https://www.googleapis.com/auth/drive.file",
        "https://www.googleapis.com/auth/drive.appdata",
        "https://www.googleapis.com/auth/drive.metadata",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "background": {
      "scripts": ["scripts/background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["http://orteil.dashnet.org/cookieclicker/", "https://orteil.dashnet.org/cookieclicker/"],
        "js": ["scripts/load_save.js"]
      }
    ],
    "browser_action": {
      "default_title": "Cookie Clicker Share",
      "default_popup": "html/login.html",
      "default_icon": {
        "16": "images/cookie-16.png",
        "32": "images/cookie-32.png",
        "48": "images/cookie-48.png",
        "128": "images/cookie-128.png"
      }
    },
    "options_page": "html/options.html",
    "icons": {
      "16": "images/cookie-16.png",
      "32": "images/cookie-32.png",
      "48": "images/cookie-48.png",
      "128": "images/cookie-128.png"
    },
    "manifest_version": 2
  }